{
  "compilerOptions": {
    "removeComments": true,
    "preserveConstEnums": true,
    "strict": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "stripInternal": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "target": "esnext",
    "sourceMap": true,
    "baseUrl": "src",
    "outDir": ".dist"
  }
}